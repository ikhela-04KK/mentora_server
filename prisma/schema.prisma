// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole{
  ADMIN
  INVITE
  TEACHER 
  STUDENT
}

model User {
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  password String
  role UserRole @default(INVITE)
  avatar String
}
